<div class="panel panel-info">
  <!-- Default panel contents -->
  <div class="panel-heading">Project list</div>

  <!-- List group -->
  <div class="list-group">
    {{#each}}
      {{#link-to 'projects.project' id class='list-group-item'}}
        <h4>{{name}}</h4>
        <p class="list-group-item-text">{{desc}}</p>
      {{/link-to}}
    {{/each}}
  </div>
</div>
